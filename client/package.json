{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "test:coverage": "NODE_OPTIONS=--max-old-space-size=8192 node ./node_modules/vitest/vitest.mjs run --config vitest.config.ts --coverage --pool=forks --poolOptions.forks.singleFork --poolOptions.forks.minForks=1 --poolOptions.forks.maxForks=1",
    "test:api": "node scripts/api-smoke.mjs",
    "test:watch": "vitest --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:visual": "playwright test --grep @visual",
    "test:e2e:a11y": "playwright test --grep @accessibility",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "check:bundle-size": "node scripts/check-bundle-size.mjs",
    "check:docs": "node scripts/check-docs.mjs",
    "lighthouse:ci": "lhci autorun"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.16",
    "axios": "^1.13.2",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^15.7.4",
    "react-router-dom": "^7.12.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "^0.15.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^2.1.9",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^25.0.1",
    "msw": "^2.12.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^2.1.9"
  }
}
